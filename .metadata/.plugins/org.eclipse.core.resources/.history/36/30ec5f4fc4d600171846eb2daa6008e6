public class YourSchedule
{
	public static void main( String[] args)
	{
		
		String class1, class2, class3, class4, teach1, teach2, teach3, teach4;
		
		class1 = "Biology";
		class2 = "Math 1";
		class3 = "ECS";
		class4 = "English";
		
		teach1 = "Dickinson";
		teach2 = "Farrar";
		teach3 = "Cummings";
		teach4 = "Brown";
		
		
		
		System.out.println("----------------------------");
		System.out.println("|Per1 |"+class1+"|"+teach1+"");
		System.out.println("|Per2 |"+class2+" |"+teach2+"");
		System.out.println("|Per3 |"+class3+"   |"+teach3+"");
		System.out.println("|Per4 |"+class4+"|"+teach4+"");
		System.out.println("----------------------------");
	
	
	}

}
