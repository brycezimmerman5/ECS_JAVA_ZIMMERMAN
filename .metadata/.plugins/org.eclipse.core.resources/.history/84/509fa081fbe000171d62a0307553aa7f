
public class EvennessMethod {
	public static void main( String[] args )
	{

		for ( int i = 0; i <= 100; i++ )
		{
			if(IsEven(i) == true)
			{
				System.out.print(" <");
			}
			if(IsDivBy3(i) ==true)
			{
				System.out.print(" =");
			}
			System.out.println(i);
		}
		
		
	}
	public static boolean IsEven(int i)
	{
		if (i % 2 == 0)
		{
			return true;	
		}
		else
		{
			return false;
		}
		
       
	}
	public static boolean IsDivBy3(int i)
	{
		if (i % 3 == 0)
		{
			return true;
		}
		else
		{
			return false;
		}

		
	}
}
