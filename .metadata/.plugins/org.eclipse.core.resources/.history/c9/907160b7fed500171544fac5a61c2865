public class UsingVariables
{
	public static void main( String[] args)
	{
		int r; 
		double e;
		String c;
		
		r = 113;
		e = 2.71828;
		c = "Computer Science";
		
		System.out.println( "This is room " + r);
		System.out.println( "e is close to " + e);
		System.out.println( "I am learning a bit about" + c);
		
	

	}
}

