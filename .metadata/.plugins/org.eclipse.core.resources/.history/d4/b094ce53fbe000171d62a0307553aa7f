
public class EvennessMethod {
	public static void main( String[] args )
	{

		for ( int i = 0; i <= 100; i++ )
		{
			IsEven(i);
			IsDivBy3(i);
			System.out.println(i);
		}
		
		
	}
	public static String IsEven(int i)
	{
		if (i % 2 == 0)
		{
			return "<";
		}
		return null;
       
	}
	public static int IsDivBy3(int i)
	{
		if (i % 3 == 0)
		{
			
		}
		return 0;
		
	}
}
